@model  IEnumerable<int>


@{
    ViewBag.Title = "Cambios";
}

<h2>Lista de cambios de estampas (Borrar si ya la cambie)</h2>

<table class="table">
    <tr>
        <th>
            Numeros
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item)
            </td>
            <td>
                @Html.ActionLink("Deshacerme de la estampita (Venderla)", "cambiar", new { id = item })                
            </td>
        </tr>
    }

</table>